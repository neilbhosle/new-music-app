{
  "name": "new-music-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://neilbhosle.github.io/new-music-app",
  "dependencies": {
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.8",
    "jest-dom": "^4.0.0",
    "json-server": "^1.0.0-alpha.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
